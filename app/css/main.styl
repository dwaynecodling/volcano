}


button.trigger
    background: url('../images/cd-icon-arrow.svg') no-repeat center center;
/*styling open close button*/
.button {
    display:inline;
    position:absolute;
    right:30px;
    top:6px;
    z-index:999;
    font-size:30px;
}
.button a {
    text-decoration:none;
}
.btn-open:after {
    color:#fff;
    content:"\f0c9";
    font-family:"FontAwesome";
    transition-property:all .2s linear 0s;
    -moz-transition:all .2s linear 0s;
    -webkit-transition:all .2s linear 0s;
    -o-transition:all .2s linear 0s;
}
.btn-open:hover:after {
    color:#f2385a;
}
.btn-close:after {
    color:#fff;
    content:"\f00d";
    font-family:"FontAwesome";
    transition-property:all .2s linear 0s;
    -moz-transition:all .2s linear 0s;
    -webkit-transition:all .2s linear 0s;
    -o-transition:all .2s linear 0s;
}
.btn-close:hover:after {
    color: #f2385a;
}
/*overlay*/
.overlay {
    display:none;
    position:fixed;
    top:0;
    height:100%;
    width:100%;
    background:#333;
    overflow:auto;
    z-index:99;
}
.wrap {
    color:#e9e9e9;
    text-align:center;
    max-width:100%;
    margin:0 auto;
}
.wrap ul.wrap-nav {
    border-bottom: 1px solid #575757;
    margin-left: -25%;
    margin-top: 10px;    
}

@media only screen and (min-width : 1025px) and (max-width : 1599px) { 
    .wrap ul.wrap-nav {
    margin-left: -11%;
    }
    }
    
.wrap ul.wrap-nav li {
    display:inline-block;
    vertical-align:top;
    position:relative;
}
.wrap ul.wrap-nav li a {
    color:#f2385a;
    display: inline-block;
    padding: 1em 3em;
    text-decoration:none;
    letter-spacing: 1px;
    transition-property:all .2s linear 0s;
    -moz-transition:all .2s linear 0s;
    -webkit-transition:all .2s linear 0s;
    -o-transition:all .2s linear 0s;
    font-size: 16px;
}
.wrap ul.wrap-nav li a:hover {
    color:#fff;
}
.wrap ul.wrap-nav ul {
    padding:20px 0;
}
.wrap ul.wrap-nav ul li {
    display:block;
    font-size:13px;
    width:100%;
    color:#e9e9e9;
}
.wrap ul.wrap-nav ul li a {
    color:#f0f0f0;
    font-size: 13px;
}
.wrap ul.wrap-nav ul li a:hover {
    text-decoration: underline;
    -moz-text-decoration-color: #f2385a;
    -webkit-text-decoration-color: #f2385a;
    text-decoration-color: #f2385a;
}
.social {
    font-size: 25px;
    padding: 20px;
    float: right
    font-size: 18px;
}
.social p {
    margin: 0;
    padding: 20px 0 5px 0;
    line-height: 30px;
    font-size: 13px;
}
.social p a {
    color:#f2385a;
    text-decoration:none;
    margin:0;
    padding:0;
}
.social-icon {
    width:80px;
    height:50px;
    background:#e9e9e9;
    color:#333;
    display:inline-block;
    margin:0 10px;
    transition-property:all .2s linear 0s;
    -moz-transition:all .2s linear 0s;
    -webkit-transition:all .2s linear 0s;
    -o-transition:all .2s linear 0s;
}
.social-icon:hover {
    background:#f2385a;
    color:#f0f0f0;
}
.social-icon i {
    margin-top:17px;
}
@media screen and (max-width:75em) {
    .wrap ul.wrap-nav>li {
        width:100%;
        padding:30px 0px;
        border-bottom:1px solid #575757;
    }
    .wrap ul.wrap-nav {
        padding:30px 0px 0px 0px;
        margin-left: 0;
    }
    nav ul {
        opacity:0;
        visibility:hidden;
    }
    .social {
        color:#c1c1c1;
        font-size:25px;
        float: none;
    }
    .social-icon {
        width:100%;
        height:50px;
        background:#fff;
        color:#333;
        display:block;
        margin:5px 0;
    }
}
.content {
    width:100%;
    margin-top:200px;
    font-size:20px;
    color: #333;
    text-align: center;
}


nav {
    
    width:100%;
    position:relative;
    height:50px;
    background:none;
}
nav ul {
    
    padding-left: 18.5%;
    padding-top: 2px;
}
nav ul li {
    display:inline-block;
    
}
nav ul li a {
    color:#fff;
    display:inline-block;
    padding:1em 3em;
    text-decoration:none;
    letter-spacing: 1px;
    transition-property:all .2s linear 0s;
    -moz-transition:all .2s linear 0s;
    -webkit-transition:all .2s linear 0s;
    -o-transition:all .2s linear 0s;
    font-size:16px;
}
nav ul li a:hover {
    color: #f2385a;
}

.nav-tel {
    display: inline;
    position: absolute;
    right: 100px;
    top: 13px;
    z-index: 999;
    font-size: 18px;
}

@media only screen and (min-width : 0px) and (max-width : 600px) { 
.nav-tel {

    display: none;
}

}
.top-phone {
    font-size: 26px;
    padding-top: 3px;
    
}

.tel-align{
    
}

